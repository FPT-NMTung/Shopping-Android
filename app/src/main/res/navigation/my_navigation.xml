<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/my_navigation"
        app:startDestination="@id/home2">
    <fragment
            android:id="@+id/home2"
            android:name="fu.prm391.sampl.project.Home"
            android:label="fragment_home"
            tools:layout="@layout/fragment_home" >
        <action
                android:id="@+id/action_home2_to_cart"
                app:destination="@id/cart"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim"
                app:popUpToInclusive="false" />
    </fragment>
    <fragment
            android:id="@+id/account"
            android:name="fu.prm391.sampl.project.Account"
            android:label="fragment_account"
            tools:layout="@layout/fragment_account" >
        <action
                android:id="@+id/action_account_to_setting"
                app:destination="@id/setting"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
            android:id="@+id/cart"
            android:name="fu.prm391.sampl.project.Cart"
            android:label="fragment_cart"
            tools:layout="@layout/fragment_cart" >
        <action
                android:id="@+id/action_cart_to_account"
                app:destination="@id/account"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
            android:id="@+id/setting"
            android:name="fu.prm391.sampl.project.Setting"
            android:label="fragment_setting"
            tools:layout="@layout/fragment_setting" >
        <action
                android:id="@+id/action_setting_to_home2"
                app:destination="@id/home2"
                app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
</navigation>